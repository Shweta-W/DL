cat mynewfile
git log
git log --oneline  (To Print list of all commits)
git commit <commitid>
gedit mynewfile 
# change there some line 
git status
git add .
git status
git commit -m "Added info on branching"
git log --oneline
git diff <latest commit id > <commit id >
git revert <second commit id >





